import { Contact } from "../entries/contact";
import { Client } from "../client";
import { Sex } from "../core/constanst";
export declare class Friend extends Contact {
    user_id: string;
    get is_self(): boolean;
    sex: Sex;
    constructor(c: Client, info: Friend.Info);
    static from(this: Client, user_id: string): Friend;
}
export declare namespace Friend {
    interface Info extends Contact.Info {
        user_id: string;
        nickname: string;
        remark: string;
        sex: Sex;
    }
}
