import { Client } from "./client";
export declare class Request {
}
export declare class FriendRequestEvent extends Request {
    private c;
    ticket: string;
    user_id: string;
    nickname: string;
    constructor(c: Client, recommend: Request.Friend);
    approve(): Promise<boolean>;
}
export declare namespace Request {
    interface Friend {
        user_id: string;
        nickname: string;
        ticket: string;
    }
}
