import { Contact } from "../entries/contact";
import { Client } from "../client";
import { Member } from "../entries/member";
export declare class Group extends Contact {
    group_id: string;
    static from(this: Client, group_id: string): Group;
    get group_name(): string;
    get member_count(): number;
    getMemberList(): Member.Info[];
    getMemberInfo(member_id: string): Member.Info;
    pickMember(member_id: string): any;
    constructor(c: Client, info: Group.Info);
}
export declare namespace Group {
    interface Info extends Contact.Info {
        group_id: string;
        group_name: string;
        member_count: number;
    }
}
